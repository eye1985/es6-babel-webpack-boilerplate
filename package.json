{
  "name": "js-starter-kit",
  "version": "1.1.1",
  "description": "A starter pack for JS applications",
  "main": "app.js",
  "author": "Erik",
  "license": "MIT",
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --progress",
    "dev": "npm run clean && cross-env NODE_ENV=development webpack -d",
    "build": "npm run clean && cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  }
}
